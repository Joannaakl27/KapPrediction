<div class="container  mt-5 mb-5">
  <div class="bg-light p-4 m-auto">
    <div class="col-lg-12 mt-4">
      <div class="bg-custom table-responsive">
        <table class="table ">
          <thead>
            <tr>
              <th scope="col">Voyage ID</th>
              <th scope="col">Service</th>
              <th scope="col">Vessel Length</th>
              <th scope="col"># Containers</th>
              <th scope="col">Discharge Ratio</th>
              <th scope="col">CI</th>
              <th scope="col">Berth Occ.</th>
              <th scope="col">Yard Occ.</th>
              <th scope="col">Cargo Ops. Time</th>
            </tr>
          </thead>
          <tbody>
            <%= @vessel_visits.each do |vessel_visit| %>
              <tr>
                <td><%= vessel_visit.voyage_number %></td>
                <td><%= vessel_visit.service_id %></td>
                <td><%= vessel_visit.vessel_length %></td>
                <td><%= vessel_visit.total_container_moves %></td>
                <td><%= vessel_visit.discharge_container_move_ratio %></td>
                <td><%= vessel_visit.crane_intensity %></td>
                <td><%= vessel_visit.berth_occupancy %></td>
                <td><%= vessel_visit.yard_occupancy %></td>
                <td><%= vessel_visit.estimated_cargo_operating_time%></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
